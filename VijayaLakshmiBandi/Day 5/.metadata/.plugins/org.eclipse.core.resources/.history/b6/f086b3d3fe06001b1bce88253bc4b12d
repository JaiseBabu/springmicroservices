package com.Assignments.XMLBasedSetterDI;

public class EmployeeSetterDI {
	private int id;
	private String name;
	private double salary;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getSalary() {
		return salary;
	}
	public void setSalary(double salary) {
		this.salary = salary;
	}
	void display() {
		system.out.println("Id is "+id+"\nEmp name is "+name+"\nSalary is"+salary
	}
}
